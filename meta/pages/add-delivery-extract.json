{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name",
                              "component": "Name",
                              "name": "NameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Name",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${deliveryextract.Name}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NameRequiredValidator",
                              "component": "Name",
                              "text": "Name is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Description",
                              "component": "Description",
                              "name": "DescriptionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 200,
                              "type": "textbox",
                              "name": "Description",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${deliveryextract.Description}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Extract",
                              "component": "ExtractId",
                              "name": "ExtractIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getExtractsForExtractIdResult}",
                              "textProperty": "Name",
                              "valueProperty": "ExtractId",
                              "placeholder": "Choose Extract",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "ExtractId",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${deliveryextract.ExtractId}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ExtractIdRequiredValidator",
                              "component": "ExtractId",
                              "text": "ExtractId is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Deliver As Set",
                              "component": "DeliverAsSet",
                              "name": "DeliverAsSetLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "bool?",
                              "type": "checkbox",
                              "name": "DeliverAsSet",
                              "triState": true,
                              "value": "${deliveryextract.DeliverAsSet}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Is Active",
                              "component": "IsActive",
                              "name": "IsActiveLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "bool",
                              "type": "checkbox",
                              "name": "IsActive",
                              "value": "${deliveryextract.IsActive}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new DeliveryExtract!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "SDE.createDeliveryExtract",
                        "parameters": [
                          {
                            "name": "DeliveryExtract",
                            "value": "${deliveryextract}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${deliveryextract}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${deliveryextract}",
                  "visible": "${deliveryextract != null}",
                  "itemType": "Sde5.Models.Sde.DeliveryExtract"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "SDE.getExtracts",
        "parameters": [],
        "then": [
          {
            "name": "getExtractsForExtractIdResult",
            "type": "set",
            "value": "${result.Value.AsODataEnumerable()}",
            "valueType": "IEnumerable<Sde5.Models.Sde.Extract>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "deliveryextract",
        "type": "set",
        "value": "new Sde5.Models.Sde.DeliveryExtract(){}",
        "valueType": "Sde5.Models.Sde.DeliveryExtract"
      }
    ]
  },
  "name": "Add Delivery Extract",
  "layout": "Main"
}