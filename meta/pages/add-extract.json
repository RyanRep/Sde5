{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name",
                              "component": "Name",
                              "name": "NameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 100,
                              "type": "textbox",
                              "name": "Name",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${extract.Name}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NameRequiredValidator",
                              "component": "Name",
                              "text": "Name is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Description",
                              "component": "Description",
                              "name": "DescriptionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 100,
                              "type": "textbox",
                              "name": "Description",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${extract.Description}"
                            },
                            {
                              "type": "required-validator",
                              "name": "DescriptionRequiredValidator",
                              "component": "Description",
                              "text": "Description is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "External Code",
                              "component": "ExternalCode",
                              "name": "ExternalCodeLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 100,
                              "type": "textbox",
                              "name": "ExternalCode",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${extract.ExternalCode}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ExternalCodeRequiredValidator",
                              "component": "ExternalCode",
                              "text": "ExternalCode is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Is Active",
                              "component": "IsActive",
                              "name": "IsActiveLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "bool",
                              "type": "checkbox",
                              "name": "IsActive",
                              "value": "${extract.IsActive}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Extract!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "SDE.createExtract",
                        "parameters": [
                          {
                            "name": "Extract",
                            "value": "${extract}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${extract}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${extract}",
                  "visible": "${extract != null}",
                  "itemType": "Sde5.Models.Sde.Extract"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "extract",
        "type": "set",
        "value": "new Sde5.Models.Sde.Extract(){}",
        "valueType": "Sde5.Models.Sde.Extract"
      }
    ]
  },
  "name": "Add Extract",
  "layout": "Main"
}