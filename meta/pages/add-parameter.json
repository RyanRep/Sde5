{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Extract",
                              "component": "ExtractId",
                              "name": "ExtractIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getExtractsForExtractIdResult}",
                              "textProperty": "Name",
                              "valueProperty": "ExtractId",
                              "placeholder": "Choose Extract",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "ExtractId",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${parameter.ExtractId}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ExtractIdRequiredValidator",
                              "component": "ExtractId",
                              "text": "ExtractId is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name",
                              "component": "Name",
                              "name": "NameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 10,
                              "type": "textbox",
                              "name": "Name",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${parameter.Name}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NameRequiredValidator",
                              "component": "Name",
                              "text": "Name is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Alias",
                              "component": "Alias",
                              "name": "AliasLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Alias",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${parameter.Alias}"
                            },
                            {
                              "type": "required-validator",
                              "name": "AliasRequiredValidator",
                              "component": "Alias",
                              "text": "Alias is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Description",
                              "component": "Description",
                              "name": "DescriptionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 100,
                              "type": "textbox",
                              "name": "Description",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${parameter.Description}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Parameter!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "SDE.createParameter",
                        "parameters": [
                          {
                            "name": "Parameter",
                            "value": "${parameter}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${parameter}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${parameter}",
                  "visible": "${parameter != null}",
                  "itemType": "Sde5.Models.Sde.Parameter"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "SDE.getExtracts",
        "parameters": [],
        "then": [
          {
            "name": "getExtractsForExtractIdResult",
            "type": "set",
            "value": "${result.Value.AsODataEnumerable()}",
            "valueType": "IEnumerable<Sde5.Models.Sde.Extract>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "parameter",
        "type": "set",
        "value": "new Sde5.Models.Sde.Parameter(){}",
        "valueType": "Sde5.Models.Sde.Parameter"
      }
    ]
  },
  "name": "Add Parameter",
  "layout": "Main"
}