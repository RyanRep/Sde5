{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "List Extracts"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "attributes": [
                    {
                      "name": "@oninput",
                      "value": "@(async(args) => {search = $\"{args.Value}\";await grid0.Reload();})"
                    }
                  ],
                  "name": "textbox0",
                  "placeholder": "Search ...",
                  "style": {
                    "display": "block",
                    "marginBottom": "10px",
                    "width": "100%"
                  },
                  "type": "textbox",
                  "value": ""
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "filterMode": "advanced",
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getListExtractsResult}",
                  "events": {
                    "loadData": [
                      {
                        "name": "SDE.getListExtracts",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": [
                              {
                                "filters": [],
                                "operator": "or"
                              },
                              {
                                "value": "${(string.IsNullOrEmpty(event.filter)? \"true\" : event.filter)}"
                              }
                            ],
                            "operator": "and"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          }
                        ],
                        "then": [
                          {
                            "name": "getListExtractsResult",
                            "type": "set",
                            "value": "${result.Value.AsODataEnumerable()}",
                            "valueType": "IEnumerable<Sde5.Models.Sde.ListExtract>"
                          },
                          {
                            "name": "getListExtractsCount",
                            "type": "set",
                            "value": "${result.Count}",
                            "valueType": "int"
                          }
                        ],
                        "type": "invoke",
                        "error": [
                          {
                            "detail": "Unable to load ListExtracts",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "ListId",
                      "title": "List Id"
                    },
                    {
                      "property": "ExtractId",
                      "title": "Extract Id"
                    }
                  ],
                  "type": "grid",
                  "count": "${getListExtractsCount}",
                  "itemType": "Sde5.Models.Sde.ListExtract"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "condition": "string.IsNullOrEmpty(search)",
        "name": "search",
        "type": "set",
        "value": "\"\"",
        "valueType": "string"
      }
    ]
  },
  "name": "List Extracts",
  "layout": "Main"
}